pipeline{
    agent any
    stages{
      stage('Build'){
          steps{
          echo 'Build Stage'
          }
      }
      stage('Stage 2'){
          steps{
          bat 'stage 2'
          }         
      }
        stage('Stage 3'){
          steps{
         echo 'Stage 3'
          }         
      }
        stage('Stage 4'){
          steps{
         echo 'Stage4'
          }         
      }
      stage('Name of stage')
      {
      steps{
      	echo "whatever you want the stage to do"
      }
      }

    }
}
